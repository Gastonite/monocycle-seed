{
  "name": "monocycle-seed",
  "version": "1.0.0",
  "description": "A seed to start developing an isomorphic application using monocycle",
  "main": "server/index.js",
  "dependencies": {
    "@cycle/dom": "^20.4.0",
    "@cycle/history": "^6.10.0",
    "@cycle/html": "^2.4.0",
    "@cycle/isolate": "^3.4.0",
    "@cycle/run": "^4.4.0",
    "@cycle/time": "^0.15.0",
    "babel-core": "^6.26.3",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "browserify-hmr": "^0.3.6",
    "csstips": "^0.3.0",
    "csx": "^9.0.0",
    "cycle-onionify": "^6.1.0",
    "envify": "^4.1.0",
    "hapi": "^17.5.3",
    "hapi-pino": "^5.0.1",
    "if-env": "^1.0.4",
    "inert": "^5.1.0",
    "module-alias": "^2.1.0",
    "monocycle": "github:Gastonite/monocycle#dev",
    "ms": "^2.1.1",
    "ramda": "^0.25.0",
    "snabbdom-merge": "github:Gastonite/snabbdom-merge#dev",
    "typestyle": "^2.0.1",
    "uglifyify": "^5.0.1",
    "xstream": "^11.7.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "_moduleAliases": {
    "component": "./src/component",
    "drivers": "./drivers",
    "components": "./src/components",
    "utilities": "./src/utilities",
    "assertions": "./node_modules/monocycle/assertions",
    "style": "./src/style"
  },
  "scripts": {
    "watch": "budo src/dev.js --pushstate --dir src --live=*.{html,css} --port 8080 -- -p browserify-hmr",
    "start": "if-env NODE_ENV=production && npm run prod || npm run dev",
    "dev": "npm run build:dev && npm run start:dev",
    "prod": "npm run build:prod && npm run start:prod",
    "start:dev": "node .",
    "start:prod": "NODE_ENV=production node server/index.js",
    "build": "browserify -e src/index.js -o public/bundle.js",
    "build:dev": "npm run build -- --debug",
    "build:prod": "NODE_ENV=production npm run build -- -g uglifyify -g envify",
    "prebuild": "rm -fr public",
    "test": "echo \"Error: no tests for the moment :)\" && exit 1"
  },
  "author": "Cl√©ment Pasquier <clement@n00sphere.net>",
  "license": "ISC",
  "devDependencies": {
    "budo": "^11.3.2",
    "cycle-restart": "^0.2.3"
  }
}
