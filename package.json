{
  "name": "isomorphic",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.js",
  "dependencies": {
    "@cycle/dom": "^20.4.0",
    "@cycle/history": "^6.10.0",
    "@cycle/html": "^2.4.0",
    "@cycle/isolate": "^3.4.0",
    "@cycle/run": "^4.4.0",
    "@cycle/time": "^0.15.0",
    "babel-core": "^6.26.3",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "browserify-hmr": "^0.3.6",
    "budo": "^11.3.2",
    "concurrently": "^3.6.1",
    "csstips": "^0.3.0",
    "csx": "^9.0.0",
    "cycle-onionify": "^6.1.0",
    "cycle-restart": "^0.2.3",
    "esm": "^3.0.80",
    "hapi": "^17.5.3",
    "hapi-pino": "^5.0.1",
    "inert": "^5.1.0",
    "module-alias": "^2.1.0",
    "monocycle": "github:Gastonite/monocycle#dev",
    "node-dev": "^3.1.3",
    "ramda": "^0.25.0",
    "snabbdom-merge": "github:Gastonite/snabbdom-merge#dev",
    "typestyle": "^2.0.1",
    "uglifyify": "^5.0.1",
    "watchify": "^3.11.0",
    "xstream": "^11.7.0"
  },
  "esm": {
    "cjs": true,
    "mode": "auto"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "_moduleAliases": {
    "component": "./src/component",
    "components": "./src/components",
    "drivers": "./src/drivers",
    "utilities": "./src/utilities",
    "assertions": "./node_modules/monocycle/assertions",
    "style": "./src/style"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run start:dev",
    "start:dev": "node .",
    "start:prod": "node server/index.js",
    "dev": "budo src/dev.js --pushstate --dir src --live=*.{html,css} --port 8080 -- -p browserify-hmr",
    "build": "browserify -e src/index.js -o public/bundle.js",
    "build:dev": "npm run build -- --debug",
    "build:dev:html": "cp src/index.html public/",
    "build:prod": "npm run build -- -g uglifyify",
    "dev2": "concurrently --kill-others npm:dev:*",
    "dev:watch": "watchify -e src/index.js -o public/bundle.js -t babelify --debug",
    "prebuild": "npm run clean",
    "clean": "rm -fr public"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {}
}
